{
  "devDependencies": {
    "fvtt-types": "github:League-of-Foundry-Developers/foundry-vtt-types#main"
  },
  "scripts": {
    "dnd5e:setup": "bash scripts/setup-dnd5e-source.sh",
    "dnd5e:cleanup": "bash scripts/cleanup-dnd5e-source.sh",
    "dnd5e:diff": "bash scripts/generate-diff.sh",
    "dnd5e:dev": "npm run dnd5e:setup && npm run dnd5e:diff"
  },
  "dnd5e": {
    "currentVersion": "release-5.0.4",
    "diff": {
      "from": "4.3.x",
      "to": "release-5.0.4"
    }
  },
  "imports": {
    "#dnd5e/*": {
      "import": "./dnd5e/*"
    }
  },
  "rootDir": "."
}
